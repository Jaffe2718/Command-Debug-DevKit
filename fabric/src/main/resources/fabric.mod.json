{
    "schemaVersion": 1,
    "id": "cmdkit",
    "version": "${version}",
    "name": "Command Debug Service",
    "description": "This is a mod that provides a method to execute and debug Minecraft commands by external programs. It is used to be a command debug connect kit between Minecraft and external programs.",
    "authors": [
        "Jaffe2718"
    ],
    "contact": {
        "homepage": "https://github.com/Jaffe2718",
        "issues": "https://github.com/Jaffe2718/Socket-Injection-Debugger/issues",
        "sources": "https://github.com/Jaffe2718/Socket-Injection-Debugger"
    },
    "license": "MIT",
    "icon": "assets/cmdkit/icon.png",
    "environment": "client",
    "entrypoints": {
        "main": ["me.jaffe2718.cmdkit.CommandDebugDevKit"],
        "client": ["me.jaffe2718.cmdkit.client.CommandDebugDevKitClient"]
    },
    "mixins": [
        "cmdkit.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "fabric": "*",
        "minecraft": "${minecraft_version}"
    }
}
