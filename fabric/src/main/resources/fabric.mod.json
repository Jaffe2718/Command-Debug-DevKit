{
    "schemaVersion": 1,
    "id": "cmdkit",
    "version": "${version}",
    "name": "Command Debug Service",
    "description": "This is a mod that provides a method to execute and debug Minecraft commands by external programs. It is used to be a command debug connect kit between Minecraft and external programs.",
    "authors": [
        "Jaffe2718"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/command-debug-service",
        "issues": "https://github.com/Jaffe2718/Command-Debug-DevKit/issues",
        "sources": "https://github.com/Jaffe2718/Command-Debug-DevKit",
        "email": "qqyttwqeei@163.com"
    },
    "license": "MIT",
    "icon": "assets/cmdkit/icon.png",
    "environment": "client",
    "entrypoints": {
        "main": ["me.jaffe2718.cmdkit.CommandDebugDevKit"],
        "client": ["me.jaffe2718.cmdkit.client.CommandDebugDevKitClient"]
    },
    "mixins": [
        "cmdkit.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.15.0",
        "fabric-lifecycle-events-v1": "*",
        "fabric-message-api-v1": "*",
        "minecraft": "${compatible_minecraft_versions}"
    }
}
